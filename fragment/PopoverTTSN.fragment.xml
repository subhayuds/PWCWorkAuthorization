<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover title="TTSN List" class="sapUiContentPadding" placement="Bottom" contentWidth="10%" contentMinWidth="10%">
		<Table items="{/Readg}">
	        <columns>
	            <Column/>
	        </columns>
	        <items>
	            <ColumnListItem>
	                <cells>
	                    <Text text="{Ttsn}"/>
	                </cells>
	            </ColumnListItem>
	        </items>
    	</Table>
	</Popover>
</core:FragmentDefinition>